    "scripts": {
        "watch": "tsc -w",
        "seedcms": "ts-node src/scripts/seed-data-cms.ts",
        "dev": "nodemon --exec 'tsc && node dist/index.js'",
        "start:dev": "concurrently \"tsc -w\" \"nodemon build/index.js\"",
        "build": "tsc && mkdir -p build/uploads",
        "lint:check": "eslint .",
        "lint:fix": "eslint --fix .",
        "start": "node build/index.js",
        "format:check": "prettier . --write",
        "typeorm": "typeorm-ts-node-commonjs",
        "schema:sync": "npx typeorm-ts-node-commonjs schema:sync -d ./src/data-source.ts",
        "typeorm:cache": "npx typeorm-ts-node-commonjs cache:clear -d ./src/data-source.ts",
        "schema:drop": "npx typeorm-ts-node-commonjs schema:drop -d ./src/data-source.ts",
        "migration:create": "npx typeorm migration:create ./src/migrations/$npm_config_name",
        "migration:generate": "npx typeorm-ts-node-commonjs migration:generate ./src/migrations/new-migration -d ./src/data-source.ts",
        "migration:run": "npx typeorm-ts-node-commonjs migration:run -d ./src/data-source.ts",
        "migration:show": "npx typeorm-ts-node-commonjs migration:show -d ./src/data-source.ts",
        "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d ./src/data-source.ts",
        "migration:sync": "npx typeorm-ts-node-commonjs schema:sync -d ./src/data-source.ts",
        "prepare": "husky"
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    }
}